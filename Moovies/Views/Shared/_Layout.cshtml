    <link href="~/scripts/lib/owl-carousel/owl.transitions.css" rel="stylesheet">
    <link href="~/scripts/lib/magnific-popup/magnific-popup.css" rel="stylesheet">
    <link href="~/scripts/lib/video/YTPlayer.css" rel="stylesheet">
    <link href="~/scripts/lib/flipclock/flipclock.css" rel="stylesheet">

