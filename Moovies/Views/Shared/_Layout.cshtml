    <script src="~/js/app.js"></script>
    <script src="~/Scripts/ng-file-upload-shim.min.js"></script>
    <script src="~/Scripts/ng-file-upload.min.js"></script>
    
    <!-- JavaScript -->
    <script src="~/scripts/lib/jquery.appear.js"></script>
    <script src="~/scripts/lib/owl-carousel/owl.carousel.min.js"></script>
    <script src="~/scripts/lib/magnific-popup/jquery.magnific-popup.min.js"></script>
    <script src="~/scripts/lib/video/jquery.mb.YTPlayer.js"></script>
    <script src="~/scripts/lib/flipclock/flipclock.js"></script>
    <script src="~/scripts/lib/jquery.animateNumber.js"></script>
    <script src="~/scripts/lib/waypoints.min.js"></script>
    <script src="~/Scripts/moment.min.js"></script>
    <script src="~/Scripts/angular-moment.min.js"></script>
    

    <!-- Custom Theme JavaScript -->
    <script src="~/scripts/main.js"></script>

